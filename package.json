{
  "name": "wakayamarb.org",
  "version": "1.0.0",
  "description": "Website of Wakayama.rb",
  "main": "index.html",
  "scripts": {
    "build:doorkeeper": "curl -sL https://api.doorkeeper.jp/groups/wakayamarb/events > ./dest/events.json",
    "build": "gulp build --verbose && cp ./index.html ./dest/index.html && cp -r ./images ./dest/images && npm run build:doorkeeper",
    "start": "gulp start",
    "lint:sass": "sass-lint",
    "lint:es": "eslint ./src/*.js",
    "lint:md": "remark .",
    "lint": "npm run lint:sass && npm run lint:es && npm run lint:md",
    "test": "mocha --harmony --compilers js:babel-core/register test/**/*.unit.js",
    "keygen": "bash ./bin/keygen.sh",
    "generate-favicons": "gulp favicons",
    "screenshot": "http-server -s & PS=$! && phantomjs ./bin/screenshot.js && kill $PS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wakayamarb/wakayamarb.org.git"
  },
  "keywords": [
    "wakayama",
    "community",
    "ruby",
    "ruby on rails"
  ],
  "author": {
    "name": "Wakayama.rb",
    "url": "http://wakayamarb.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wakayamarb/wakayamarb.org/issues"
  },
  "homepage": "https://github.com/wakayamarb/wakayamarb.org#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.26.0",
    "babelify": "^10.0.0",
    "browser-sync": "^3.0.2",
    "browserify": "^17.0.0",
    "eslint": "^8.57.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-favicons": "^4.0.1",
    "gulp-less": "^5.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^5.1.0",
    "gulp-sketch": "^1.0.5",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "http-server": "^14.1.1",
    "mocha": "^10.3.0",
    "phantomjs-prebuilt": "^2.1.13",
    "remark-cli": "^12.0.0",
    "remark-lint": "^9.1.2",
    "remark-preset-lint-recommended": "^6.1.3",
    "sass": "^1.71.1",
    "sass-lint": "^1.13.1",
    "should": "^13.2.3",
    "streamqueue": "^1.1.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended"
    ]
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "highlight.js": "^11.9.0",
    "moment": "^2.30.1",
    "superagent": "^8.1.2"
  }
}
